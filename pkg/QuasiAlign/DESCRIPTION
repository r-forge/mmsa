Package: QuasiAlign
Type: Package
Title: Infrastructure for Quasi-Alignment of Genetic Sequences
Version: 0.0-4
Date: 20xx-xx-xx
Authors@R: c(person("Michael", "Hahsler", role = c("aut", "cre", "cph"),
	    email = "mhahsler@lyle.smu.edu"),
	person("Anurag", "Nagar", role = c("aut","cph"),
